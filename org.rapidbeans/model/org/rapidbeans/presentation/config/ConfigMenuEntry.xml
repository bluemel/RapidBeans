<beantype name="org.rapidbeans.presentation.config.ConfigMenuEntry"
	abstract="true">
	<property name="name"
		key="true"
	/>
	<property name="rolesrequired"
		type="choice"
		enum="org.rapidbeans.core.basic.RapidEnum"
		multiple="true"
	/>
	<property name="enabler"
		type="association"
		composition="true"
		targettype="org.rapidbeans.presentation.enabler.Enabler"
		maxmult="1"
		collectionclass="java.util.ArrayList"
	/>
</beantype>
