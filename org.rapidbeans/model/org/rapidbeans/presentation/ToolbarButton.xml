<beantype name="org.rapidbeans.presentation.ToolbarButton">
	<property name="name"/>
	<property name="action"
		type="association"
		composition="true"
		targettype="org.rapidbeans.service.Action"
		maxmult="1"
		collectionclass="java.util.ArrayList"
		/>
	<property name="enabler"
		type="association"
		composition="true"
		targettype="org.rapidbeans.presentation.enabler.Enabler"
		maxmult="1"
		collectionclass="java.util.ArrayList"
		/>
	<codegen mode="joint" implementation="strict"/>
</beantype>
