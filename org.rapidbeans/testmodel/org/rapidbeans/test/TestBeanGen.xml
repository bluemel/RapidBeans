<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<beantype name="org.rapidbeans.test.TestBeanGen">
	<property name="surname"
		key="true"
		maxlength="32"
		minlength="3"
	/>
	<property name="prename"
		key="true"
	/>
	<property name="dateofbirth"
		type="date"
		key="true"
		default="19641014"
		maxval="20101231"
		minval="19000101"
	/>
	<property name="email"
		pattern="\A[.\-0-9A-Za-z]*@[.\-0-9A-Za-z]*\z"
	/>
	<property name="city"
		type="string"
		must="true"
		default="Ismaning"
	/>
	<property name="zipcode"
		type="integer"
		default="85737"
		maxval="99999"
		minval="10000"
	/>
	<property name="shoesize"
		valuetype = "Byte"
		type="integer"
		default="43"
		minval="20"
		maxval="55"
	/>
	<property name="sex"
		type="choice"
		enum="org.rapidbeans.domain.org.Sex"
		default="male"
	/>
	<property name="married"
		type="boolean"
		default="true"
		mandatory="true"
	/>
	<property name = "languages"
		type="choice"
		default="german,english,french"
		multiple="true"
		enum="LangGeneric"
	/>
	<property name="height"
		type="quantity"
		quantity="org.rapidbeans.domain.math.Length"
		default="1.73 m"
		maxval="3.00 m"
		minval="0.30 m"
	/>
	<property name="homedir"
		type="file"
		filetype="directory"
		default="."
	/>
	<property name="lang"
		type="choice"
		multiple="true"
		enum="LangGeneric"
		default="german,english"
	/>
	<property name="version"
		type="version"
		mandatory="true"
		default="2.0"
	/>
	<property name="webaddress"
		type="url"
		mandatory="true"
		default="http://www.rapidbeans.org"
	/>

<!--
	<property name="zipcode"
		type="zipcode"
		default="85737"
	/>
	<property name="iq"
		type="float"
		default="187.654321"
		maxval="200.0"
		minval="50.0"
	/>
	<property name="dec"
		type="decimal"
		default="3.141"
		maxval="9999999999.9999999999"
		minval="-9999999999.9999999999"
	/>
-->
	<property name="children"
		type="association"
		inverse="father"
		singular="child"
		targettype="org.rapidbeans.test.TestBeanGen"
		minmult="0"
		maxmult="4"
		mandatory="true"
	/>
	<property name="father"
		type="association"
		inverse="children"
		targettype="org.rapidbeans.test.TestBeanGen"
		minmult="1"
		maxmult="1"
		mandatory="true"
	/>
	<codegen mode="none"/>
</beantype>
