<?xml version="1.0" encoding="ISO-8859-1" standalone="yes"?>
<applicationcfg
	name="Test"
	rootpackage="org.rapidbeans"
	applicationclass="org.rapidbeans.presentation.TestClient"
	guitype="swing"
	defaultlocale="en"
	defaultcharset="UTF-8"
	lookandfeel="metal"
	>
	<locale
		id="en"
		name="en"
		/>
	<action
		id="org.rapidbeans.service.ActionQuit"
		classname="org.rapidbeans.service.ActionQuit"
		waitcursor="true"
		background="false"
		progressbar="false"
		/>
	<mainwindow
		name="mainwindow"
		width="800"
		height="500"
		>
		<menubar
			name="menubar"
			>
			<menu
				name="file"
				>
				<menuitem
					name="load"
					/>
				<menuitem
					name="save"
					/>
				<menuitem
					name="quit"
					action="org.rapidbeans.service.ActionQuit"
					/>
			</menu>
			<menu
				name="edit"
				>
				<menuitem
					name="view"
					/>
				<submenu
					name="insert"
					>
					<menuitem
						name="object1"
						/>
					<menuitem
						name="object2"
						/>
				</submenu>
				<submenu
					name="test2"
					>
					<menuitem
						name="test21"
						/>
					<submenu
						name="test22"
						>
						<menuitem
							name="test221"
							/>
						<menuitem
							name="test222"
							/>
					</submenu>
					<menuitem
						name="test23"
						/>
				</submenu>
			</menu>
			<menu
				name="test"
				>
				<menuitem
					name="test1"
					/>
			</menu>
		</menubar>
	</mainwindow>
	<currency
		restricttoone="true"
		uselocal="true"
		/>
	<xmlbinding
		/>
</applicationcfg>
