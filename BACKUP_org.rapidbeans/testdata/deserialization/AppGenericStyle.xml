<?xml version="1.0" encoding="ISO-8859-1" standalone="yes"?>
<rb:bean xmlns:rb="http://rapidbeans.org/core/basic"
	rb:type="org.rapidbeans.presentation.config.ConfigApplication"
	name="Test"
	rootpackage="org.rapidbeans"
	applicationclass="org.rapidbeans.presentation.TestClient"
	guitype="swing"
	lookandfeel="windows"
	defaultlocale="en"
	defaultcharset="UTF-8"
	>
	<locale
		id="de"
		name="de"
		/>
	<locale
		id="en"
		name="en"
		/>
	<mainwindow
		name="mainwindow"
		width="800"
		height="500"
		>
		<menubar
			name="menubar"
			>
			<menu
				name="file"
				>
				<menuentry
					rb:type="org.rapidbeans.presentation.config.ConfigMenuItem"
					name="load"
					/>
				<menuentry
					rb:type="org.rapidbeans.presentation.config.ConfigMenuItem"
					name="save"
					/>
				<menuentry
					rb:type="org.rapidbeans.presentation.config.ConfigMenuItem"
					name="quit"
					>
					<action
						classname="org.rapidbeans.service.ActionQuit"
						/>
				</menuentry>
			</menu>
			<menu
				name="edit"
				>
				<menuentry
					rb:type="org.rapidbeans.presentation.config.ConfigMenuItem"
					name="view"
					/>
				<menuentry
					rb:type="org.rapidbeans.presentation.config.ConfigSubmenu"
					name="insert"
					>
					<menuentry
						rb:type="org.rapidbeans.presentation.config.ConfigMenuItem"
						name="object1"
						/>
					<menuentry
						rb:type="org.rapidbeans.presentation.config.ConfigMenuItem"
						name="object2"
						/>
				</menuentry>
			</menu>
			<menu
				name="test"
				>
				<menuentry
					rb:type="org.rapidbeans.presentation.config.ConfigMenuItem"
					name="test1"
					/>
				<menuentry
					rb:type="org.rapidbeans.presentation.config.ConfigSubmenu"
					name="test2"
					>
					<menuentry
						rb:type="org.rapidbeans.presentation.config.ConfigMenuItem"
						name="test21"
						/>
					<menuentry
						rb:type="org.rapidbeans.presentation.config.ConfigSubmenu"
						name="test22"
						>
						<menuentry
							rb:type="org.rapidbeans.presentation.config.ConfigMenuItem"
							name="test221"
							/>
						<menuentry
							rb:type="org.rapidbeans.presentation.config.ConfigMenuItem"
							name="test222"
							/>
					</menuentry>
					<menuentry
						rb:type="org.rapidbeans.presentation.config.ConfigMenuItem"
						name="test23"
						/>
				</menuentry>
			</menu>
		</menubar>
	</mainwindow>
	<currency
		restricttoone="true"
		uselocal="true"
		/>
</rb:bean>
